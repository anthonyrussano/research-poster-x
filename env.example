# Base URL for your local LM Studio server (OpenAI-compatible endpoint)
LMSTUDIO_BASE_URL=http://localhost:1234/v1

# Model name served by LM Studio
LMSTUDIO_MODEL=lmstudio-community/Meta-Llama-3-8B-Instruct

# API key passed to the OpenAI-compatible client (LM Studio accepts anything)
OPENAI_API_KEY=lm-studio

# Optional fallback env var checked by the script if OPENAI_API_KEY is absent
API_KEY=lm-studio

# Android device IP or serial for uiautomator2 (e.g., 192.168.0.50:5555)
GALAXY_IP=192.168.0.50:5555

